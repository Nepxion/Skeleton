<?xml version="1.0" encoding="UTF-8"?>
<skeleton>
    <group>
        <key>project</key>
        <label>工程配置</label>
        <description></description>
        <entity>
            <key>moduleName</key>
            <label>工程模块名</label>
            <description>moduleName</description>
            <note>【必改项】首字母必须小写，中间只允许出现“-”</note>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>basePackage</key>
            <label>上层包路径</label>
            <description>basePackage</description>
            <note>【必改项】该路径会作为所有Java代码的上层路径。moduleName、basePackage和projectType三者组合起来解析出相关目录和结构规则，例如moduleName=payment-ccb，basePackage=com.nepxion，projectType=server，那么工程名为payment-ccb-server，类路径为com.nepxion.payment.ccb.server
            </note>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
    </group>
    <group>
        <key>pom</key>
        <label>POM配置</label>
        <description></description>
        <entity>
            <key>pomGroupId</key>
            <label>Group Id</label>
            <description></description>
            <note></note>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>pomArtifactId</key>
            <label>Artifact Id</label>
            <description></description>
            <note></note>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>pomName</key>
            <label>Name</label>
            <description></description>
            <note></note>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>pomVersion</key>
            <label>Version</label>
            <description></description>
            <note></note>
            <type>COMBOBOX</type>
            <options>1.0.0</options>
            <highlightable>false</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>springCloudVersion</key>
            <label>Spring Cloud Version</label>
            <description></description>
            <note></note>
            <type>COMBOBOX</type>
            <options>Dalston.SR4;Edgware.RELEASE</options>
            <highlightable>false</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>springBootVersion</key>
            <label>Spring Boot Version</label>
            <description></description>
            <note></note>
            <type>COMBOBOX</type>
            <options>1.5.6.RELEASE</options>
            <highlightable>false</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>javaVersion</key>
            <label>Java Version</label>
            <description></description>
            <note></note>
            <type>COMBOBOX</type>
            <options>1.8;1.7;1.6</options>
            <highlightable>false</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
    </group>
    <group>
        <key>framework</key>
        <label>框架配置</label>
        <description></description>
        <entity>
            <key>serviceName</key>
            <label>服务名</label>
            <description></description>
            <note>【必改项】Spring Cloud服务名</note>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>port</key>
            <label>端口</label>
            <description></description>
            <note>【必改项】Spring Cloud端口号</note>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>eurekaEnabled</key>
            <label>Eureka组件</label>
            <description></description>
            <note></note>
            <type>CHECKBOX</type>
            <highlightable>false</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>eurekaUrl</key>
            <label>Eureka地址</label>
            <description></description>
            <note>【必改项】Eureka URL地址</note>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
    </group>
</skeleton>