<?xml version="1.0" encoding="UTF-8"?>
<skeleton>
    <group>
        <key>project</key>
        <label>工程配置</label>
        <description>工程配置</description>
        <entity>
            <key>basePackage</key>
            <label>上层包路径(basePackage)</label>
            <description>【必改项】该路径会作为所有Java代码的上层路径。moduleName、basePackage和projectType三者组合起来解析出相关目录和结构规则，例如moduleName=payment-ccb，basePackage=com.nepxion，projectType=server，那么工程名为payment-ccb-server，类路径为com.nepxion.payment.ccb.server
            </description>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>moduleName</key>
            <label>工程的模块名(moduleName)</label>
            <description>【必改项】首字母必须小写，中间只允许出现“-”</description>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>pomGroupId</key>
            <label>Pom Group Id</label>
            <description>【必改项】Pom Group Id</description>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>pomArtifactId</key>
            <label>Pom Artifact Id</label>
            <description>【必改项】Pom Artifact Id</description>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>pomName</key>
            <label>Pom Name</label>
            <description>【必改项】Pom Name</description>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>pomVersion</key>
            <label>Pom Version</label>
            <description>Pom Version</description>
            <type>COMBOBOX</type>
            <options>1.0.0</options>
            <highlightable>false</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>springCloudVersion</key>
            <label>Spring Cloud Version</label>
            <description>Spring Cloud Version</description>
            <type>COMBOBOX</type>
            <options>Dalston.SR4;Edgware.RELEASE</options>
            <highlightable>false</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>springBootVersion</key>
            <label>Spring Boot Version</label>
            <description>Spring Boot Version</description>
            <type>COMBOBOX</type>
            <options>1.5.6.RELEASE</options>
            <highlightable>false</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>javaVersion</key>
            <label>Java Version</label>
            <description>Spring Boot Version</description>
            <type>COMBOBOX</type>
            <options>1.8;1.7;1.6</options>
            <highlightable>false</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
    </group>
    <group>
        <key>framework</key>
        <label>框架配置</label>
        <description>框架配置</description>
        <entity>
            <key>serviceName</key>
            <label>服务名(serviceName)</label>
            <description>【必改项】Spring Cloud服务名</description>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>port</key>
            <label>端口(prot)</label>
            <description>【必改项】Spring Cloud端口号</description>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>eurekaEnabled</key>
            <label>Eureka组件(eurekaEnabled)</label>
            <description>Eureka组件(推荐)</description>
            <type>CHECKBOX</type>
            <highlightable>false</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
        <entity>
            <key>eurekaUrl</key>
            <label>Eureka地址(eurekaUrl)</label>
            <description>【必改项】Eureka地址</description>
            <type>TEXTFIELD</type>
            <highlightable>true</highlightable>
            <defaultable>false</defaultable>
            <emptiable>false</emptiable>
            <editable>true</editable>
        </entity>
    </group>
</skeleton>